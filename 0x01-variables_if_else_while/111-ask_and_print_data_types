#include <stdio.h>

/**
* main - Print the nubers entered in the command promt
*        float
*	 int
*	 char
*        does nothing else
* Return: zero
*/


#include <stdio.h>
int main()
{
    float f;
    int i;
    char ch;

    printf("\nEnter value of float\n");
    scanf("%f", &f);

    printf("\nEnter value of int\n");
    scanf("%d", &i);

    printf("\nEnter value of char\n");
    scanf(" %c", &ch);

    printf("\nValue of float : %f", f);
    printf("\nValue of int : %d", i);
    printf("\nValue of char : %c", ch);
    printf("\nValue of float(rounded) : %.2f\n", f);

    return 0;
}
